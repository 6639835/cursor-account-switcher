{
  "name": "cursor-switcher-tauri",
  "private": true,
  "version": "1.1.9",
  "type": "module",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "sync-version": "node scripts/sync-version.js",
    "version": "npm run sync-version && git add -A",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run",
    "test:frontend:ci": "bash scripts/test-ci.sh",
    "test:frontend:watch": "vitest",
    "test:frontend:ui": "vitest --ui",
    "test:frontend:coverage": "vitest run --coverage",
    "test:backend": "cd src-tauri && cargo test",
    "test:backend:verbose": "cd src-tauri && cargo test -- --nocapture",
    "lint": "npm run lint:ts && npm run lint:rust",
    "lint:ts": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:rust": "cd src-tauri && cargo clippy -- -D warnings",
    "format": "npm run format:ts && npm run format:rust",
    "format:ts": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:rust": "cd src-tauri && cargo fmt",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\" && cd src-tauri && cargo fmt --check"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.3",
    "date-fns": "^3.0.6",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.9",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.0"
  }
}
